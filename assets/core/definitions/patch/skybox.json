{
  "group_name": "skybox",
  "source_name": "skybox.fx",

  "entrypoints": {
    "main_vs": {
      "stage": "vertex",

      "vertex_state": {
        "input_layout": "$auto",

        "generic_input": {
          "dynamic_compression": true,
          "position": true,
          "skinned": true,
          "normal": true,
          "tangents": true,
          "tangents_unflagged": true,
          "color": true,
          "texture_coords": true
        }
      },

      "defines": [
        [ "SKYBOX_USE_EMISSIVE_MAP", "0" ]
      ]
    },

    "main_ps": {
      "stage": "pixel",

      "pixel_state": {
        "lighting": false
      },

      "static_flags": [
        "SKYBOX_USE_EMISSIVE_MAP"
      ]
    }
  },

  "rendertypes": {
    "skybox": {
      "static_flags": {
        "SKYBOX_USE_EMISSIVE_MAP": false
      }
    },

    "skybox.emissive": {
      "static_flags": {
        "SKYBOX_USE_EMISSIVE_MAP": true
      }
    }
  },

  "states": {
    "unlit opaque": {
      "type": "standard",

      "vertex_shader": {
        "entrypoint": "main_vs"
      },

      "pixel_shader": {
        "entrypoint": "main_ps"
      }
    },

    "unlit opaque hardedged": {
      "type": "standard",

      "vertex_shader": {
        "entrypoint": "main_vs"
      },

      "pixel_shader": {
        "entrypoint": "main_ps"
      }
    },

    "unlit transparent": {
      "type": "standard",

      "vertex_shader": {
        "entrypoint": "main_vs"
      },

      "pixel_shader": {
        "entrypoint": "main_ps"
      }
    },

    "unlit transparent hardedged": {
      "type": "standard",

      "vertex_shader": {
        "entrypoint": "main_vs"
      },

      "pixel_shader": {
        "entrypoint": "main_ps"
      }
    }
  }
}